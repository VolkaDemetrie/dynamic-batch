<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.volka.dynamicbatch.mapper.job.SchdMapper">

    <select id="selectAll" resultType="Schd">
        SELECT
            schd_cd
            ,schd_cron
            ,schd_nm
        FROM cbm_schd
        ORDER BY schd_cd ASC
    </select>
</mapper>


